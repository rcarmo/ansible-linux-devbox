	stretch			= TRUE
      }
    image 
      {
     function			= BOX
	recolorable		= TRUE
	detail				= "hruler"
	file					= "Images/Others/ruler.gif"
	border			= { 2, 2, 2, 2 }
	stretch			= TRUE
      }
  }
}
